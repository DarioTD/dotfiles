Xft.dpi: 96
Xft.antialias: 1
Xft.hinting: 1
Xft.autohint: 1
Xft.rgba: rgba
Xft.hintstyle: hintmedium
Xft.lcdfilter: lcdfilter

!!URxvt.intensityStyles: false
!!URxvt.depth: 32
URxvt.font: xft:LiberationMono:size=16, \
            xft:Liberation Mono Font:size=16, xft:FontAwesome:size=15 \
            xft:Monospace:style=Medium:antialias=false
!!URxvt.saveLines: 5000
URxvt.scrollBar: false

!! Extensions
!!URxvt.perl-ext-common: default,matcher,resize-font,url-select,keyboard-select,clipboard
! keyboard-select
!!URxvt.keysym.M-escape: perl:keyboard-select:activate
! resize-font
!!URxvt.resize-font.smaller: C-j
!!URxvt.resize-font.bigger: C-k
! matcher
!!URxvt.url-launcher: /usr/bin/xdg-open
!!URxvt.matcher.button: 1
! clipboard
!!URxvt.keysym.M-c: perl:clipboard:copy
!!URxvt-keysym.M-v: perl:clipboard:paste

!! Theme
*.foreground:   #c5c8c6
*.background:   #1e2120
*.cursorColor:  #c5c8c6
*.cursorBlink:  1
! black
*.color0:       #1d2019
*.color8:       #969896
! red
*.color1:       #dc322f
*.color9:       #dc322f
! green
*.color2:       #859900
*.color10:      #859900
! yellow
*.color3:       #b58900
*.color11:      #b58900
! blue
*.color4:       #26a0d2
*.color12:      #26a0d2
! magenta
*.color5:       #6c71c4
*.color13:      #6c71c4
! cyan
*.color6:       #2aa198
*.color14:      #2aa198
! white
*.color7:       #93a1a1
*.color15:      #fdf6e3
